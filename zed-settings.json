  //     "model": "gpt-3.5-turbo"
  //   },
  //   "version": "2",
  //   "enabled": true
  // },
  "features": {
    "inline_completion_provider": "copilot"
  },
  "terminal": {
    // "button": false,
    "dock": "right"
  }
}
